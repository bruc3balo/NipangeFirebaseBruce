<?xml version="1.0" encoding="utf-8"?>
<!--&lt;!&ndash;<androidx.constraintlayout.widget.ConstraintLayout -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".login.Log_in"




    android:scaleType="centerCrop"
    android:gravity="center_horizontal"
    android:adjustViewBounds="true"
    android:orientation="vertical"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:adjustViewBounds="true"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:scaleType="centerCrop">




        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="160dp"

            android:src="@drawable/ic_backward_arrow"

            />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="100dp"
            android:layout_gravity="center"
            android:layout_marginRight="50dp"
            android:text="Hello There!"
            android:textColor="#d11e48"
            android:textSize="33sp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="18dp"
            android:layout_marginRight="45dp"
            android:fontFamily="@font/maven_pro_regular"
            android:text="Please Log in to continue."
            android:textColor="#444444"
            android:textSize="18dp" />


        <com.google.android.material.textfield.TextInputLayout

            android:id="@+id/editText090"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:layout_gravity="center"
            android:backgroundTint="#b9b9b9"
            android:textColor="#d3d3d3"
            android:textColorHint="#d3d3d3">

            <!--            android:background="@drawable/logininput"-->
            <EditText
                android:id="@+id/editText"
                android:layout_width="match_parent"
                android:layout_height="50sp"
                android:layout_marginTop="180dp"
                android:backgroundTint="#b9b9b9"
                android:textColor="#444444"
                android:layout_marginBottom="12dp"


                android:ems="10"
                android:hint="Email Address"
                android:inputType="textPersonName"
                android:textColorHint="#d3d3d3"

                />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout

            android:id="@+id/editText091"
            android:layout_width="300dp"
            android:layout_height="64dp"
            android:backgroundTint="#b9b9b9"
            android:layout_gravity="center"
            android:textColor="#444444"
            android:textColorHint="#d3d3d3">

            <!--            android:background="@drawable/logininput"-->
            <EditText
                android:id="@+id/editText2"
                android:layout_width="match_parent"
                android:layout_height="50sp"
                android:layout_marginBottom="12dp"
                android:backgroundTint="#b9b9b9"
                android:textColor="#444444"
                android:ems="10"
                android:hint="Password"
                android:inputType="textPassword"
                android:textColorHint="#d3d3d3" />
        </com.google.android.material.textfield.TextInputLayout>


        <Button
            android:id="@+id/button"
            android:layout_width="180dp"
            android:layout_height="50sp"
            android:layout_marginTop="43dp"
            android:layout_marginRight="60dp"
            android:background="@drawable/login_button"
            android:text="Login"
            android:layout_gravity="center"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="24dp"
            android:layout_marginLeft="115dp"
            android:layout_marginTop="-35dp"
            android:text="Need Help?"
            android:layout_gravity="center"
            android:textColor="#F23B5F"
            android:textSize="15sp" />

        <LinearLayout
            android:layout_marginTop="50dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:layout_gravity="center"
            android:padding="10dp">

            <ImageButton
                android:layout_margin="10dp"
                android:src="@drawable/ic_facebook_"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:padding="25dp"
                android:background="@drawable/button_round_bg"
                android:layout_weight="1"/>

            <ImageButton
                android:layout_margin="10dp"
                android:layout_width="0dp"
                android:padding="25dp"
                android:src="@drawable/ic_twitter_"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@drawable/button_round_bg"
                android:layout_weight="1"/>

            <ImageButton
                android:layout_margin="10dp"
                android:src="@drawable/ic_google_plus"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:padding="25dp"
                android:background="@drawable/button_round_bg"
                android:layout_weight="1"/>


        </LinearLayout>

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="0dp"
            android:layout_marginRight="0dp"
            android:src="@drawable/repeats"
            />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="29dp"
            android:layout_marginRight="45dp"
            android:orientation="horizontal"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="You are not a member?"
                android:textColor="#253A4B" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Register"
                android:textColor="#F23B5F" />

        </LinearLayout>




        <!--        android:layout_marginStart="16dp"-->
        <!--        android:layout_marginLeft="16dp"-->
        <!--        android:layout_marginTop="25dp"-->
        <!--        android:layout_marginEnd="365dp"-->
        <!--        android:layout_marginRight="365dp-->
        <!--         />-->
    </LinearLayout>
    <!--</androidx.constraintlayout.widget.ConstraintLayout>-->
</RelativeLayout>